<Window x:Class="TetrisPlayerWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TetrisPlayerWPF"
        xmlns:tetris="clr-namespace:TetrisDXControll;assembly=TetrisDXControll"
        mc:Ignorable="d"
        Title="MainWindow" Height="601" Width="949">
    <Grid x:Name="Grid">
        <WindowsFormsHost VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
            <tetris:TetrisDX x:Name="DXPanel" Anchor="Top" Dock="Left"></tetris:TetrisDX>
        </WindowsFormsHost>
    </Grid>
</Window>
